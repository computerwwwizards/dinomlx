{
  "name": "dinomlx",
  "version": "1.0.0",
  "type": "module",
  "bin": {
    "dinomlx": "./src/index.ts"
  },
  "publishConfig": {
    "bin": {
      "dinomlx": "./dist/index.js"
    },
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "require": "./dist/index.cjs"
      }
    },
    "main": "./dist/index.cjs",
    "types": "./dist/index.d.ts"
  },
  "main": "./src/index.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rslib build",
    "check": "biome check --write",
    "dev": "rslib build --watch",
    "format": "biome format --write",
    "lint": "eslint .",
    "test": "vitest run",
    "example": "tsx src/index.ts build --src-root example/src --out-dir example/dist --cache-dir example/.dinomlx/cache"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@eslint/js": "^9.39.1",
    "@rslib/core": "^0.19.5",
    "@types/node": "^24.10.9",
    "csstype": "^3.2.3",
    "eslint": "^9.39.1",
    "globals": "^16.5.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0",
    "vitest": "^4.0.18"
  },
  "private": true,
  "peerDependencies": {
    "node-html-parser": "^7.0.2"
  },
  "dependencies": {
    "@computerwwwizards/dependency-injection": "^1.8.1",
    "@types/glob": "^9.0.0",
    "cheerio": "^1.2.0",
    "commander": "^14.0.3",
    "glob": "^13.0.5"
  }
}
